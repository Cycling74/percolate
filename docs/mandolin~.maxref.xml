	<!--MESSAGES-->
	<methodlist>
		<method name="bang">
			<arglist>
				<arg name="ARG_NAME_0" type="bang" optional="0" />
			</arglist>
			<digest>
				Pluck the mandolin
			</digest>
		</method>
		<method name="float">
			<arglist>
				<arg name="ARG_NAME_0" type="float" optional="0" />
			</arglist>
			<digest>
				Function depends on inlet
			</digest>
			<description>
				In left inlet: Controls pluck amplitude.
				<br/>
				In 2nd inlet: Controls pluck position.
				<br/>
				In 3rd inlet: Controls string damping.
				<br/>
				In 4th inlet: Controls detuning.
				<br/>
				In 5th inlet: Controls body size.
				<br/>
				In 6th inlet: Controls frequency.
			</description>
		</method>
		<method name="signal">
			<arglist />
			<digest>
				Function depends on inlet
			</digest>
			<description>
				In left inlet: Controls pluck amplitude.
				<br/>
				In 2nd inlet: Controls pluck position.
				<br/>
				In 3rd inlet: Controls string damping.
				<br/>
				In 4th inlet: Controls detuning.
				<br/>
				In 5th inlet: Controls body size.
				<br/>
				In 6th inlet: Controls frequency.
			</description>
		</method>
		<method name="mic">
			<arglist>
				<arg name="ARG_NAME_0" type="list" optional="0" />
			</arglist>
			<digest>
				Microphone position
			</digest>
			<description>
				An integer 0-11 will select one of the NBody microphone setups.
			</description>
		</method>
	</methodlist>
