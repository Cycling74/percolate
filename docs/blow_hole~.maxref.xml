<!--Data marked TEXT_HERE, INLET_TYPE, OUTLET_TYPE, ARG_NAME and ARG_TYPE need to be filled in-->
<c74object name="blow_hole~" module="" category="">
	<digest>
		Model a one-tonehole clarinet
	</digest>
	<description>
		STK clarinet physical model with one register hole and one tonehole.
	</description>
	<!--METADATA-->
	<metadatalist>
		<metadata name="author">
			Dan Trueman and R. Luke DuBois
		</metadata>
	</metadatalist>
	<!--INLETS-->
	<inletlist>
		<inlet id="0" type="INLET_TYPE">
			<digest>
				control messages
			</digest>
			<description>
				TEXT_HERE
			</description>
		</inlet>
	</inletlist>
	<!--OUTLETS-->
	<outletlist>
		<outlet id="0" type="OUTLET_TYPE">
			<digest>
				output (signal)
			</digest>
			<description>
				TEXT_HERE
			</description>
		</outlet>
	</outletlist>
	<!--ARGUMENTS-->
	<objarglist>
	</objarglist>
	<!--MESSAGES-->
	<methodlist>
		<method name="control">
			<arglist>
				<arg name="ARG_NAME_0" type="list" optional="0" />
			</arglist>
			<digest>
				Send control messages
			</digest>
			<description>
				Control messages must have two arguments, control number and control value (0-128).
				<br/>
				<br/>
				Control numbers:
				<br/>
				1 = register state
				<br/>
				2 = reed stiffness
				<br/>
				4 = noise gain
				<br/>
				11 = tonehole state
				<br/>
				128 = breath pressure
			</description>
		</method>
		<method name="noteoff">
			<arglist>
				<arg name="ARG_NAME_0" type="list" optional="0" />
			</arglist>
			<digest>
				Note off
			</digest>
			<description>
				TEXT_HERE
			</description>
		</method>
		<method name="noteon">
			<arglist>
				<arg name="ARG_NAME_0" type="list" optional="0" />
			</arglist>
			<digest>
				Play a note
			</digest>
			<description>
				The message <m>noteon</m> followed by a pitch (Hz) and amplitude (0-1) will play a note.
			</description>
		</method>
	</methodlist>
	<!--ATTRIBUTES-->
	<!--EXAMPLE-->
	<examplelist>
		<example caption="" img="" />
	</examplelist>
