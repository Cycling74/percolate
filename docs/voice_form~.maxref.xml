	<methodlist>
		<method name="float">
			<arglist>
				<arg name="ARG_NAME_0" type="float" optional="0" />
			</arglist>
			<digest>
				TEXT_HERE
			</digest>
			<description>
				TEXT_HERE
			</description>
		</method>
		<method name="signal">
			<arglist />
			<digest>
				TEXT_HERE
			</digest>
			<description>
				TEXT_HERE
			</description>
		</method>
		<method name="control">
			<arglist>
				<arg name="ARG_NAME_0" type="list" optional="0" />
			</arglist>
			<digest>
				Send control messages
			</digest>
			<description>
				Control messages must have two arguments, control number and control value (0-128).
				<br/>
				<br/>
				Control numbers:
				<br/>
				1 = vibrato gain
				<br/>
				2 = voice/unvoiced mix
				<br/>
				4 = vowel/phoneme selection
				<br/>
				11 = vibrato freq
				<br/>
				128 = loudness (spectral tilt)
			</description>
		</method>
		<method name="noteoff">
			<arglist>
				<arg name="ARG_NAME_0" type="list" optional="0" />
			</arglist>
			<digest>
				Note off
			</digest>
			<description>
				TEXT_HERE
			</description>
		</method>
		<method name="noteon">
			<arglist>
				<arg name="ARG_NAME_0" type="list" optional="0" />
			</arglist>
			<digest>
				Play a note
			</digest>
			<description>
				The message <m>noteon</m> followed by a pitch (Hz) and amplitude (0-1) will play a note.
			</description>
		</method>
	</methodlist>
