<!--Data marked TEXT_HERE, INLET_TYPE, OUTLET_TYPE, ARG_NAME and ARG_TYPE need to be filled in-->
<c74object name="prc_fm~" module="" category="">
	<digest>
		Variety of FM instruments
	</digest>
	<description>
		A variety of classic FM instruments ported from the STK by Perry Cook.
	</description>
	<!--METADATA-->
	<metadatalist>
		<metadata name="author">
			Dan Trueman and R. Luke DuBois
		</metadata>
		<metadata name="tag">
			TEXT_HERE
		</metadata>
	</metadatalist>
	<!--INLETS-->
	<inletlist>
		<inlet id="0" type="INLET_TYPE">
			<digest>
				control messages
			</digest>
			<description>
				TEXT_HERE
			</description>
		</inlet>
		<inlet id="1" type="INLET_TYPE">
			<digest>
			</digest>
			<description>
				TEXT_HERE
			</description>
		</inlet>
	</inletlist>
	<!--OUTLETS-->
	<outletlist>
		<outlet id="0" type="OUTLET_TYPE">
			<digest>
				output (signal)
			</digest>
			<description>
				TEXT_HERE
			</description>
		</outlet>
	</outletlist>
	<!--ARGUMENTS-->
	<objarglist>
	</objarglist>
	<!--MESSAGES-->
	<methodlist>
		<method name="type">
			<arglist>
				<arg name="ARG_NAME_0" type="list" optional="0" />
			</arglist>
			<digest>
				Set instrument type
			</digest>
			<description>
				<m>type</m> followed by an integer (1-7) will set the instrument type.
				<br/>
				1 = BeeThree
				<br/>
				2 = FMVoices
				<br/>
				3 = HevyMetl
				<br/>
				4 = PercFlut
				<br/>
				5 = Rhodey
				<br/>
				6 = TubeBell
				<br/>
				7 = Wurley
			</description>
		</method>
		<method name="control">
			<arglist>
				<arg name="ARG_NAME_0" type="list" optional="0" />
			</arglist>
			<digest>
				Send control messages
			</digest>
			<description>
				Control messages must have two arguments, control number and control value (0-128).
				<br/>
				<br/>
				Control numbers:
				<br/>
				1 = LFO depth
				<br/>
				2 = control one
				<br/>
				4 = control two
				<br/>
				11 = LFO speed
				<br/>
				128 = ADSR targets
			</description>
		</method>
		<method name="noteoff">
			<arglist>
				<arg name="ARG_NAME_0" type="list" optional="0" />
			</arglist>
			<digest>
				Note off
			</digest>
			<description>
				TEXT_HERE
			</description>
		</method>
		<method name="noteon">
			<arglist>
				<arg name="ARG_NAME_0" type="list" optional="0" />
			</arglist>
			<digest>
				Play a note
			</digest>
			<description>
				The message <m>noteon</m> followed by a pitch (Hz) and amplitude (0-1) will play a note.
			</description>
		</method>
	</methodlist>
	<!--ATTRIBUTES-->
