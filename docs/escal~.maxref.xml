	<objarglist>
		<objarg id="0" name="rounding-algorithm" type="OBJARG_TYPE" optional="1">
			<digest>
				Rounding algorithm
			</digest>
			<description>
				-1 = always round down
				<br/>
				0 = round normally (>= 0.5 round up, otherwise down)
				<br/>
				1 always round up
			</description>
		</objarg>
