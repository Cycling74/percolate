<c74object name="escal~" module="" category="">
	<digest>
		Signal rounder
	</digest>
	<description>
		Round an incoming signal to the nearest integer. Multiple rounding algorithms are available.
	</description>
	<!--METADATA-->
	<metadatalist>
		<metadata name="author">
			Dan Trueman and R. Luke DuBois
		</metadata>
		<metadata name="tag">
